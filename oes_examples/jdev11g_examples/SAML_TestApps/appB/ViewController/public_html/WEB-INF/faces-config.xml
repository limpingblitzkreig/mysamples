<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
  </application>
  <navigation-rule>
    <from-view-id>/appBStart.jspx</from-view-id>
    <navigation-case>
      <from-outcome>res</from-outcome>
      <to-view-id>/aut/restricted.jspx</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <managed-bean>
    <managed-bean-name>Security</managed-bean-name>
    <managed-bean-class>nl.whitehorses.appB.beans.Security</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
</faces-config>
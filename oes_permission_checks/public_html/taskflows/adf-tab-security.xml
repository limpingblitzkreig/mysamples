<?xml version="1.0" encoding="US-ASCII" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="adf-tab-security">
    <default-activity id="__1">TabView</default-activity>
    <data-control-scope id="__2">
      <shared/>
    </data-control-scope>
    <managed-bean>
      <managed-bean-name>oesSecurityBean</managed-bean-name>
      <managed-bean-class>oracle.iam.examples.adfsecurity.beans.OESSecurityExampleBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="TabView">
      <page>/SecurityView.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
